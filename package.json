    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "angular-svg-icon": "^13.0.0",
    "apexcharts": "^3.35.3",
    "boxicons": "^2.1.4",
    "izitoast": "^1.4.0",
    "ng-apexcharts": "^1.7.1",
    "ng-dynamic-component": "^10.7.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.10.4",
