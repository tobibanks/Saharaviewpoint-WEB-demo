{
  "name": "saharaviewpoint-web",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "client-app": "nx serve client-app",
    "build-client-staging": "npx nx build client-app --configuration=staging --output-path=dist/staging/client",
    "build-admin-staging": "npx nx build admin-app --configuration=staging --output-path=dist/staging/admin",
    "build-dev": "npm run build-client-staging && npm run build-admin-staging",
    "build-client-prod": "npx nx build client-app --configuration=production --output-path=dist/production/client",
    "build-admin-prod": "npx nx build admin-app --configuration=production --output-path=dist/production/admin",
    "build-prod": "npm run build-client-prod && npm run build-admin-prod",
    "watch": "nx build --watch --configuration development",
    "test": "nx test"
  },
  "private": true, 
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/common": "^17.0.0",
